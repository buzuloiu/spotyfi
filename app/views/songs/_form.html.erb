
  <%= form_for (@song) do |f| %>



  <%= f.label :title%>
  <%= f.text_field :title,  class: 'form-control'%>

  <%= f.label :artist_id%>
  <%= f.text_field :artist_id,  class: 'form-control'%>

  <%= f.label "File" %>
  <%= f.file_field :url %>


  <%= f.submit yield(:button_text), class: "btn btn-primary" %>

  <% end %>

  <%= link_to 'cancel', songs_path %>
