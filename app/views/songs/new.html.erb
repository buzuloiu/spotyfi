<h1>New Song</h1>

<%= form_tag :action => 'create' do %>
<p><label for = "song_title">Title</label>:
<%= text_field 'song', 'title' %></p>

<p><label for = "song_artist_id">Artis</label>:
<%= text_field 'song', 'artist_id' %></p>

<p><label for = "song_album_id">Album</label>:
<%= text_field 'song', 'album_id' %></p>

<p><label for = "song_genre_id">Genre</label><br/>
<%= text_field 'song', 'genre_id' %></p>

<p><label for = "song_file">File</label><br/>
<%= file_field 'song', 'file' %></p>

<%= submit_tag "Create" %>

<% end -%>

<%= link_to 'Back', songs_path %>
